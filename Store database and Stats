--display the database ordered by price. 
Select * from store
Order by Price; 

--display the database ordered by aisle. 
Select * from store
Order by aisle;

--display the database group by section. 
Select * from store
group by section;

--what is the sum price of items in the store? 
Select Sum (price) from store;
